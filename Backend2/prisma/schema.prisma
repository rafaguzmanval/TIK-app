generator client {
  provider = "prisma-client-js"
}



datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Client {
  id Int @id @default(autoincrement())
  name String @unique
  email String @unique
  password String
  projects Project[]

}

model Project {
  id Int @id @default(autoincrement())
  client Client @relation(fields: [authorId], references: [id])
  authorId Int
  plots Plot[]
  name String
  description String



  @@unique([authorId, name], name: "Identifier")
}

model Plot {
  id Int @id @default(autoincrement())
  plotId Int
  name String

  project Project @relation(fields: [plotId], references: [id])

} 

model Log {
  id Int @id @default(autoincrement())

}

model Tree {
  id Int @id @default(autoincrement())

}


model Table {
  id Int @id @default(autoincrement())

}

model TIK_Tree {
  id Int @id @default(autoincrement())

}

model TIK_Timber {
  id Int @id @default(autoincrement())
}






